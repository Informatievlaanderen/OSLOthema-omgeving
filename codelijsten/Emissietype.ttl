@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://data.vlaanderen.be/id/conceptscheme/Emissietype> a skos:ConceptScheme;
    skos:prefLabel "Emissietype" ;
    skos:definition "Een type van een emissie" .

<https://data.vlaanderen.be/id/concept/Emissietype/Diffuus> a skos:Concept; 
    skos:prefLabel "Diffuse emissie" ;
    skos:definition "Een diffuse emissie, in een andere vorm dan vanuit een puntbron, in de lucht, bodem of water, alsmede in enig product." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/Geleid> a skos:Concept;
    skos:prefLabel "Geleide emissie" ;
    skos:definition "Een emissie afkomstig van een schoorsteen waarvoor bepaalde fysische kenmerken bestaan (ligging, afmetingen) en waarvan het debiet kan worden bepaald." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/FugitieveEmissie> a skos:Concept;
    skos:prefLabel "Fugitieve emissie" ;
    skos:definition "De emissie van vluchtige organische stoffen (exclusief methaan) door lekverliezen van apparaten en leiding (onderdelen)." ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/AbnormaleEmissie> a skos:Concept;
    skos:prefLabel "Abnormale emissie" ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/AndereNietGeleideEmissie> a skos:Concept;
    skos:prefLabel "Andere niet-geleide emissie" ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/DoorzetEmissie> a skos:Concept;
    skos:prefLabel "Doorzet emissie" ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/Lekverliezen> a skos:Concept;
    skos:prefLabel "Lekverliezen" ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .

<https://data.vlaanderen.be/id/concept/Emissietype/FakkelEmissie> a skos:Concept;
    skos:prefLabel "Fakkelemissie" ;
    skos:inscheme <https://data.vlaanderen.be/id/conceptscheme/Emissietype> ;
    skos:topConceptOf <https://data.vlaanderen.be/id/conceptscheme/Emissietype> .
